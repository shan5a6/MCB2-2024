pipeline {
  /*agent any/slave1/dockeragent/k8s */
  agent any
  stages {

    stage('git checkout') {
      steps {
        script {
          println "Hello all welcome to pipelinescripting"
          var1=20
          println "myvar1 value is ${var1}"
          
        }
      }
    }
  }
}
